({

  baseUrl: "../public/js",

  paths: {
    "jquery": "vendor/jquery",
    "underscore": "vendor/underscore",
    "backbone": "vendor/backbone",
    "hitch": "vendor/hitch",
    "text": "vendor/text"
  },

  shim: {
    "jquery": { exports: "$" },
    "underscore": { exports: "_" },
    "backbone": { exports: "Backbone", deps: [ 'jquery', 'underscore' ] },
    "hitch": { exports: "Hitch", deps: [ 'jquery', 'underscore', 'backbone' ] }
  },

  name: "main",

  out: "<%= out %>",

  wrap: true

})